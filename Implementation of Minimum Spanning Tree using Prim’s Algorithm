#include <stdio.h>
#include <limits.h>
#define MAX 100

int main() {
    int n = 5;
    int graph[MAX][MAX] = {
        {0, 2, 0, 6, 0},
        {2, 0, 3, 8, 5},
        {0, 3, 0, 0, 7},
        {6, 8, 0, 0, 9},
        {0, 5, 7, 9, 0}
    };
    int key[MAX], parent[MAX], inMST[MAX] = {0};

    for (int i = 0; i < n; i++) key[i] = INT_MAX;
    key[0] = 0;
    parent[0] = -1;

    for (int count = 0; count < n - 1; count++) {
        int u = -1, min = INT_MAX;
        for (int v = 0; v < n; v++)
            if (!inMST[v] && key[v] < min) {
                min = key[v];
                u = v;
            }
        inMST[u] = 1;
        for (int v = 0; v < n; v++) {
            if (graph[u][v] && !inMST[v] && graph[u][v] < key[v]) {
                key[v] = graph[u][v];
                parent[v] = u;
            }
        }
    }

    printf("Edge \tWeight\n");
    for (int i = 1; i < n; i++)
        printf("%d - %d \t%d\n", parent[i], i, graph[i][parent[i]]);
    return 0;
}
